package exp4;

public class Calculator {
	
	public int calculator(int num) {
		return num*num;
	}
	
	public double calculator(double num) {
		return num*num;
	}
	
	public int calculator(int num1, int num2) {
		return num1+num2;
	}
	
	public double calculator(double num1, double num2) {
		return num1+num2;
	}
	
	public int calculator(int num1, int num2, int num3) {
		return num1*num2*num3;
	}
	
	public double calculator(double num1, double num2, double num3) {
		return num1*num2*num3;
	}
	
	public static void main(String[] args) {
		Calculator cal = new Calculator();
		
		System.out.println("Sqaure of 5 : " + cal.calculator(5));
		System.out.println("Sqaure of 5.5 : " + cal.calculator(5.5));
		
		System.out.println("Sum of 15 and 19 : " + cal.calculator(15, 19));
		System.out.println("Sum of 13.2 and 1.9 : " + cal.calculator(13.2, 1.9));
		
		System.out.println("Product of 5 , 7 and 9 : " + cal.calculator(5, 7, 9));
		System.out.println("Product of 4.5 , 2.7 and 3.9 : " + cal.calculator(4.5, 2.7, 3.9));
	}

}
