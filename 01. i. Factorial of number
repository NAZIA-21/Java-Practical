package Exp1;

import java.util.*;

public class Fact {

	public static void main(String[] args) {
		int fact=1;
		int num;
		
		Scanner sc= new Scanner(System.in);
		System.out.print("Enter the number : ");
		num = sc.nextInt();
		
		for(int i=1; i<= num ; i++) {
			fact = fact * i;
		}
		
		System.out.println("Factorial of " + num + " is : " +fact);
	
	}
}
